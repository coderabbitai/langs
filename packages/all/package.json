{
  "name": "@ast-grep/all-langs",
  "version": "0.0.0",
  "description": "Exports all officially supported languages.",
  "keywords": ["ast-grep"],
  "homepage": "https://github.com/ast-grep/langs/tree/main/packages/all",
  "bugs": {
    "url": "https://github.com/ast-grep/langs/issues"
  },
  "license": "ISC",
  "author": {
    "name": "CodeRabbit",

  },
  "files": ["dist", "!dist/**/*.test.*"],
  "main": "dist/index.js",
  "repository": "github:ast-grep/langs",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist docs node_modules tsconfig.tsbuildinfo",
